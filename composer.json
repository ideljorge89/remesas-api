{
  "name": "symfony/website-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A skeleton to start a new Symfony website",
  "require": {
    "php": "^7.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-soap": "*",
    "api-platform/core": "^2.5",
    "consolibyte/quickbooks": "^3.6",
    "doctrine/doctrine-fixtures-bundle": "*",
    "excelwebzone/recaptcha-bundle": "1.5.13",
    "friendsofsymfony/rest-bundle": "^3.0",
    "friendsofsymfony/user-bundle": "*",
    "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
    "incenteev/composer-parameter-handler": "~2.0",
    "knplabs/knp-paginator-bundle": "*",
    "laminas/laminas-soap": "^2.8",
    "lexik/jwt-authentication-bundle": "^2.10",
    "liip/imagine-bundle": "*",
    "onurb/excel-bundle": "*",
    "quickbooks/v3-php-sdk": ">=5.0.0",
    "sensio/framework-extra-bundle": "^5.1",
    "sermepa/sermepa": "^1.1",
    "setasign/fpdi": "1.6.1",
    "setasign/fpdi-tcpdf": "1.6.1",
    "spatie/pdf-to-image": "^1.4",
    "spraed/pdf-generator-bundle": "1.7.*",
    "st/flagiconcss-bundle": "~1.0",
    "stof/doctrine-extensions-bundle": "*",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/intl": "*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "*",
    "symfony/security-bundle": "*",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*",
    "twig/extensions": "*",
    "whiteoctober/tcpdf-bundle": "*"
  },
  "require-dev": {
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
