            <td width="175px" align="left" style="font-size: 11px">{{ transferencia.numeroTarjeta }}</td>
            <td width="100x" align="left" style="font-size: 11px">{{ transferencia.importe | number_format(2) }} {{ transferencia.moneda }}</td>
        </tr>
    {% endif %}
</table>
<table>
    <tr>
        <td>&nbsp;</td>
    </tr>
</table>
<table cellpadding="3px" width="785px" style="border: 1px solid black">
    <tr>
        <td width="150px" align="left" style="font-size: 11px"></td>
