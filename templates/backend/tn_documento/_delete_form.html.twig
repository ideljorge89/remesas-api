<form method="post" action="{{ path('tn_documento_delete', {'id': tn_documento.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tn_documento.id) }}">
    <button class="btn">Delete</button>
</form>
