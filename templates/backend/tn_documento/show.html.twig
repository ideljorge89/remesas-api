{% extends 'base.html.twig' %}

{% block title %}TnDocumento{% endblock %}

{% block body %}
    <h1>TnDocumento</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tn_documento.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ tn_documento.url }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ tn_documento.estado }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ tn_documento.created ? tn_documento.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{{ tn_documento.updated ? tn_documento.updated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('tn_documento_index') }}">back to list</a>

    <a href="{{ path('tn_documento_edit', {'id': tn_documento.id}) }}">edit</a>

    {{ include('tn_documento/_delete_form.html.twig') }}
