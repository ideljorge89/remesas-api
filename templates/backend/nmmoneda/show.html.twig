{% extends 'base_backend.html.twig' %}

{% block title %}moneda{% endblock %}

{% block body %}
    <h1>moneda</h1>

    <table class=" table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ moneda.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ moneda.nombre }}</td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ moneda.created ? moneda.created|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        <tr>
            <th>Updated</th>
            <td>{{ moneda.updated ? moneda.updated|date('Y-m-d H:i:s') : '' }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('moneda_index') }}">back to list</a>

    <a href="{{ path('moneda_edit', {'id': moneda.id}) }}">edit</a>

    {{ include('backend/nmmoneda/_delete_form.html.twig') }}
{% endblock %}
